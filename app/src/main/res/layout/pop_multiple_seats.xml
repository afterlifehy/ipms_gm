<?xml version="1.0" encoding="utf-8"?>
<com.aries.ui.view.radius.RadiusLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    app:rv_backgroundColor="@color/white"
    app:rv_radius="@dimen/dp_10">

    <com.aries.ui.view.radius.RadiusRelativeLayout
        android:id="@+id/rrl_parkingNoNone"
        android:layout_width="@dimen/dp_92"
        android:layout_height="wrap_content"
        android:padding="@dimen/dp_13"
        app:rv_topLeftRadius="@dimen/dp_10"
        app:rv_topRightRadius="@dimen/dp_10">

        <TextView
            android:id="@+id/tv_parkingNoNone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginEnd="@dimen/dp_15"
            android:layout_toStartOf="@+id/cb_street"
            android:ellipsize="end"
            android:includeFontPadding="false"
            android:lines="1"
            android:text="无"
            android:textColor="@color/black"
            android:textSize="@dimen/sp_19" />

        <ImageView
            android:id="@+id/iv_parkingNoNone"
            android:layout_width="@dimen/dp_21"
            android:layout_height="@dimen/dp_21"
            android:layout_alignParentEnd="true"
            android:src="@mipmap/ic_parking_street_unchecked" />
    </com.aries.ui.view.radius.RadiusRelativeLayout>

    <View
        android:id="@+id/view_liner"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:background="@color/color_ffcccccc" />

    <com.aries.ui.view.radius.RadiusRelativeLayout
        android:id="@+id/rrl_parkingNo"
        android:layout_width="@dimen/dp_92"
        android:layout_height="wrap_content"
        android:padding="@dimen/dp_13">

        <TextView
            android:id="@+id/tv_parkingNo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginEnd="@dimen/dp_15"
            android:layout_toStartOf="@+id/cb_street"
            android:ellipsize="end"
            android:includeFontPadding="false"
            android:lines="1"
            android:textColor="@color/black"
            android:textSize="@dimen/sp_19"
            tools:text="无" />

        <ImageView
            android:id="@+id/iv_parkingNo"
            android:layout_width="@dimen/dp_21"
            android:layout_height="@dimen/dp_21"
            android:layout_alignParentEnd="true"
            android:src="@mipmap/ic_parking_street_unchecked" />
    </com.aries.ui.view.radius.RadiusRelativeLayout>

    <View
        android:id="@+id/view_liner2"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:background="@color/color_ffcccccc" />

    <com.aries.ui.view.radius.RadiusRelativeLayout
        android:id="@+id/rrl_parkingNo2"
        android:layout_width="@dimen/dp_92"
        android:layout_height="wrap_content"
        android:padding="@dimen/dp_13"
        app:rv_bottomLeftRadius="@dimen/dp_10"
        app:rv_bottomRightRadius="@dimen/dp_10">

        <TextView
            android:id="@+id/tv_parkingNo2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginEnd="@dimen/dp_15"
            android:layout_toStartOf="@+id/cb_street"
            android:ellipsize="end"
            android:includeFontPadding="false"
            android:lines="1"
            android:textColor="@color/black"
            android:textSize="@dimen/sp_19"
            tools:text="无" />

        <ImageView
            android:id="@+id/iv_parkingNo2"
            android:layout_width="@dimen/dp_21"
            android:layout_height="@dimen/dp_21"
            android:layout_alignParentEnd="true"
            android:src="@mipmap/ic_parking_street_unchecked" />
    </com.aries.ui.view.radius.RadiusRelativeLayout>
</com.aries.ui.view.radius.RadiusLinearLayout>